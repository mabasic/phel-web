{
    "name": "mabasic/phel-web",
    "description": "Getting started Web template for Phel lang.",
    "license": "MIT",
    "keywords": [
        "phel",
        "project",
        "template"
    ],
    "authors": [
        {
            "name": "Mario Bašić",
            "email": "mario@laravelista.hr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "phel-lang/phel-lang": "^0.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "repl": "vendor/bin/phel repl",
        "test": "vendor/bin/phel test",
        "format": "vendor/bin/phel format",
        "start": "vendor/bin/phel run",
        "dev": "php -S localhost:8000 -t ./public/"
    }
}
